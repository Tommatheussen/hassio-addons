{
  "name": "Firefly",
  "version": "dev",
  "slug": "firefly",
  "description": "A free and open source personal finance manager",
  "url": "https://github.com/hassio-addons/addon-firefly-iii",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "ingress": false,
  "ingress_port": 1337,
  "startup": "application",
  "homeassistant": "0.101.0b0",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "map": [
    "ssl"
  ],
  "ports": {
    "80/tcp": 3473
   },
  "ports_description": {
    "80/tcp": "Web interface"
  },
  "options": {
    "database": "sqlite",
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
   },
  "schema": {
    "database": "match(^(sqlite|mysql)$)",
    "mysql_host": "str?",
    "mysql_database": "str?",
    "mysql_user": "str?",
    "mysql_password": "str?",
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)?",
    "certfile": "str",
    "keyfile": "str",
    "ssl": "bool"
  }
  "image": "hassioaddons/firefly-iii-{arch}"
}
